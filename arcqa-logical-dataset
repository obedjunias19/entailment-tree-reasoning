    "ans_df = pd.read_csv('/kaggle/working/arcqa_logical_qa.csv')\n",
    "\n",
    "for index, row in ans_df.iterrows():\n",
    "    print(\"--- Row\", index, \"---\")\n",
    "    print(\"question:\", row['question'])\n",
    "    print(\"correct_combinations:\", row['correct_combinations'])\n",
